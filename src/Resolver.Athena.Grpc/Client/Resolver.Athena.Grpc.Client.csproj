<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Generated Client for the Resolver Athena Client</Description>
  </PropertyGroup>

  <ItemGroup>
    <Protobuf Include="$(ProtoDefinitionsFolder)/athena/athena.proto" GrpcServices="Client" ProtoRoot="$(ProtoDefinitionsFolder)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Models\Resolver.Athena.Grpc.Models.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.Net.Client" />
    <PackageReference Include="Google.Protobuf" />
    <PackageReference Include="Grpc.Tools">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>

